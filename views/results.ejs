<% console.log(movies, 'apple') %>
<% movies.forEach(movie => { %>
  <div class="movieResults">
    <h2><%= movie.Title %></h2>
    <p><%= movie.Year %></p>
    <p><%= movie.Type %></p>
    <a href="/details/<%= movie.imdbID %>">more info</a>
  </div>
  <% if (movie.Poster != "N/A") { %>
    <img  src="<%= movie.Poster %>" alt="<%= movie.Title %>'s movie poster"/> 
  <% } %>
<% }) %>

